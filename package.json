{
  "name": "timeago-react",
  "version": "2.0.1",
  "description": "timeago-react is a simple(only 1kb) react component used to format date with `*** time ago` statement. eg: '3 hours ago'.",
  "main": "lib/timeago-react.js",
  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf lib",
    "test": "npm run lint && npm run jest",
    "build": "npm run clean && cross-env NODE_ENV=production babel src -d lib && npm run build-demos && npm run test",
    "build-demos": "webpack",
    "prepublish": "npm run build",
    "jest": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hustcc/timeago-react.git"
  },
  "keywords": [
    "react",
    "component",
    "timeago-react",
    "react-timeago",
    "timeago.js",
    "timeago",
    "react-component"
  ],
  "author": "hustcc (http://github.com/hustcc)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hustcc/timeago-react/issues"
  },
  "homepage": "https://github.com/hustcc/timeago-react",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-es3-member-expression-literals": "*",
    "babel-plugin-transform-es3-property-literals": "*",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "cross-env": "^1.0.8",
    "css-loader": "~0.16.0",
    "enzyme": "^2.8.2",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "~0.8.4",
    "jest": "^19.0.2",
    "jsx-loader": "~0.13.2",
    "prop-types": "^15.0.0",
    "react": "^15.5.0",
    "react-adsense": "^0.0.1",
    "react-dom": "^15.5.0",
    "react-router": "^2.0.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.0",
    "style-loader": "~0.12.3",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "timeago.js": "^4.0.0-beta.2"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": ">=0.13.2 || ^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "jest": {
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/timeago-react.*"
    ]
  }
}
