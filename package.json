{
  "name": "timeago-react",
  "version": "3.0.0",
  "description": "timeago-react is a simple(only 1kb) react component used to format date with `*** time ago` statement. eg: '3 hours ago'.",
  "main": "dist/index.js",
  "types" : "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf dist",
    "test": "npm run lint --scripts-prepend-node-path && npm run jest",
    "build": "npm run clean && tsc && npm run build-demos && npm run test",
    "build-demos": "webpack",
    "prepublish": "npm run build",
    "jest": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hustcc/timeago-react.git"
  },
  "keywords": [
    "react",
    "component",
    "timeago-react",
    "react-timeago",
    "timeago.js",
    "timeago",
    "react-component"
  ],
  "author": "hustcc (http://github.com/hustcc)",
  "contributors": [
    "Lee Seungmin <me@2sm.kr> (https://github.com/yooonspace)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hustcc/timeago-react/issues"
  },
  "homepage": "https://github.com/hustcc/timeago-react",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.0",
    "@types/react": "^16.9.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^1.0.8",
    "css-loader": "~0.16.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "~0.8.4",
    "jest": "^24.9.0",
    "jsx-loader": "~0.13.2",
    "prop-types": "^15.0.0",
    "react": "^16.11.0",
    "react-adsense": "^0.0.1",
    "react-dom": "^16.11.0",
    "rimraf": "^2.5.0",
    "style-loader": "~0.12.3",
    "typescript": "^3.7.2",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "timeago.js": "^4.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": ">=0.13.2 || ^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "jest": {
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/timeago-react.*"
    ]
  }
}
